{
  "name": "agilecrm",
  "description": "Enjoy ServiceNow Integration on YellowAnt",
  "keywords": ["python3", "django2","yellowant"],
  "env": {
    "YELLOWANT_DEVELOPER_TOKEN": {
      "description": "YellowAnt Developer Token",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku App name",
      "required": true
    },
    "DISABLE_COLLECTSTATIC": {
      "required": false,
      "value": "1"
    },
    "ENV": {
      "required": false,
      "value": "HEROKU"
    }
  }
}
